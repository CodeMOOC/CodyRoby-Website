---
import favIcon from '~/assets/favicons/codyroby-icon.ico';
import favIconSvg from '~/assets/favicons/codyroby.svg';
import appleTouchIcon from '~/assets/favicons/codyroby.png';
---

<link rel="shortcut icon" href={favIcon} />
<link rel="icon" type="image/svg+xml" href={favIconSvg.src} />
<link rel="mask-icon" href={favIconSvg.src} color="#8D46E7" />
<link rel="logo" sizes="180x180" href={appleTouchIcon.src} />
